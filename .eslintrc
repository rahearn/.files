{
    "rules": {
        "indent": [
            2,
            4
        ],
        "quotes": [
            2,
            "single"
        ],
        "linebreak-style": [
            2,
            "unix"
        ],
        "semi": [
            2,
            "always"
        ],
        "comma-dangle": [
            1,
            "always-multiline"
        ],
        "no-console": [ 0 ]
    },
    "env": {
        "node": true,
        "browser": true,
        "requirejs": true
    },
    "extends": "eslint:recommended"
}
